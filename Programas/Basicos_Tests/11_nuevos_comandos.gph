g := graph[
  ("A", [("B",99),("C",8)]),
  ("B", [("A",99),("C",7)]),
  ("C", [("A",8),("B",7)]),
  ("Y", [("Z",7)]),
  ("Z", [("Y",7)])
];

visualize(g, "Iteradores");

print "Nodos:";
forNodes n in g do
    print n
end;

print "Vecinos de A hasta 2 niveles:";
forNeighbors x in g from "A" upto 2 do
    print x
end;

print "Pesos de las aristas del grafo:";
forEdges e in g do
    print getWeight(e)
end;

print "Aristas incidentes a A:";
forIncident e in g on "A" do
    print e
end;


print "Componentes conexas del grafo:";
forComponent h in g do
    print h
end;

nodes := getNodes(g);
print "nodes:";
print nodes;

for node in nodes do
    print "Nodo";
    print node
end