    ("5", [("2",33), ("4",44)] )
    ];

NodosVisitados := newlista()
Q := newcola()

Q:= encolar Q “1”
NodosVisitados := addLista NodosVisitados “1”

while not (IsEmptyList NodosVisitados)
	nodo = desencolar Q
	
	for c in (conexion nodo)
		if (InList NodosVisitados nodo := False) then
			Q:= encolar Q c
			NodosVisitados:= addLista NodosVisitados c
